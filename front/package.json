{
  "name": "monolith-mybatis",
  "version": "0.0.0",
  "private": true,
  "description": "Description for monolithMybatis",
  "license": "UNLICENSED",
  "scripts": {
    "prettier:check": "prettier --check \"{,src/**/,webpack/}*.{md,json,yml,html,cjs,mjs,js,ts,tsx,css,scss,vue,java}\"",
    "prettier:format": "prettier --write \"{,src/**/,webpack/}*.{md,json,yml,html,cjs,mjs,js,ts,tsx,css,scss,vue,java}\"",
    "lint": "vue-cli-service lint --no-fix",
    "lint:fix": "vue-cli-service lint",
    "cleanup": "rimraf target/classes/static/",
    "clean:lib": "rimraf node_modules",
    "clean-www": "rimraf target/classes/static/app/{src,target/}",
    "start": "vite",
    "start-tls": "pnpm run webapp:dev -- --env.tls",
    "serve": "pnpm run start",
    "build": "cross-env NODE_ENV=production NODE_OPTIONS=--max-old-space-size=8192 vite build && esno ./build/script/postBuild.ts",
    "build:analyze": "cross-env NODE_OPTIONS=--max-old-space-size=8192 REPORT=true vite build",
    "jest": "jest --coverage --logHeapUsage --maxWorkers=2 --no-cache --config jest.conf.js",
    "jest:update": "pnpm run jest -- --updateSnapshot",
    "pretest": "pnpm run lint",
    "test": "pnpm run jest",
    "gen:icon": "esno ./build/generate/icon/index.ts",
    "reinstall": "rimraf pnpm-lock.yaml && rimraf yarn.lock && rimraf package.lock.json && rimraf node_modules && pnpm run install",
    "test-ci": "pnpm run lint && pnpm run jest:update",
    "test:watch": "pnpm run jest -- --watch",
    "webapp:build": "pnpm run clean-www && pnpm run webapp:build:dev",
    "webapp:build:dev": "cross-env NODE_ENV=development NODE_OPTIONS=--max-old-space-size=8192 vite build && esno ./build/script/postBuild.ts",
    "webapp:build:prod": "pnpm run build",
    "build:docker": "vite build --mode docker",
    "components:build": "pnpm --filter @begcode/components build",
    "webapp:dev": "pnpm run start",
    "webapp:prod": "pnpm run clean-www && pnpm run webapp:build:prod",
    "webapp:test": "pnpm run test"
  },
  "config": {
    "default_environment": "prod"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
